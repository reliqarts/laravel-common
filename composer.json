{
  "name": "reliqarts/laravel-common",
  "description": "Reliq Arts' base/common classes and functions for Laravel 6.",
  "keywords": ["laravel", "simple", "common", "helpers", "functions", "global"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "replace": {
    "reliqarts/simple-commons": "1.*"
  },
  "require": {
    "php": "^7.4",
    "illuminate/support": "^5.7|^6.0",
    "monolog/monolog": "^1.24",
    "ext-json": "*",
    "spatie/laravel-sitemap": "^5.5",
    "anhskohbo/no-captcha": "^3.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "codeception/aspect-mock": "^3.0",
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReliqArts\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ReliqArts\\ServiceProvider"
      ],
      "aliases": {
        "CommonDateHelper": "ReliqArts\\Helper\\Date",
        "CommonVersionProvider": "ReliqArts\\Service\\VersionProvider"
      }
    }
  }
}
