{
  "name": "reliqarts/laravel-common",
  "description": "Reliq Arts' base/common classes and functions for Laravel 5.",
  "keywords": ["laravel", "simple", "common", "helpers", "functions", "global"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "replace": {
    "reliqarts/simple-commons": "1.*"
  },
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "^5.7",
    "monolog/monolog": "^1.24",
    "ext-json": "*",
    "illuminate/filesystem": "^5.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "codeception/aspect-mock": "^3.0",
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\": "src/",
      "ReliqArts\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ReliqArts\\ServiceProvider"
      ],
      "aliases": {
        "CommonVersionProvider": "ReliqArts\\Services\\VersionProvider"
      }
    }
  }
}
